{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "CMake: Configure Release",
			"type": "shell",
			"command": "cmake",
			"args": [
				"-S",
				".",
				"-B",
				"build",
				"-DCMAKE_BUILD_TYPE=Release"
			],
			"problemMatcher": [],
			"group": "build"
		},
		{
			"label": "CMake: Build Release",
			"type": "shell",
			"command": "cmake",
			"args": [
				"--build",
				"build",
				"--config",
				"Release"
			],
			"problemMatcher": [],
			"group": "build"
		}
	]
}